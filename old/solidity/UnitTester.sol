pragma solidity 0.4.21;

import "./Interfaces/ERC20Interface.sol";
import "./Interfaces/TokenLockupInterface.sol";

contract UnitTester {
	bytes constant private lockup = hex"";

	address public lockupContract;
	
	TokenLockupInterface public lockupI;

	event ContractCreated(address _contractAddress);
	event TestPassed(string _testName);

	function deployLockup() {
		require(lockupContract == address(0));
		bytes memory _code = lockup;
		address a;
		assembly {
			a := create(0, add(_code, 0x20), mload(_code))
		}
		lockupContract = a;
		emit ContractCreated(a);
		lockupI = TokenLockupInterface(a);
	}

	function TestDepositStake(
		uint256 _rtcToStake,
		uint256 _durationInWeeks)
		external
		returns (bool)
	{
		require(lockupI.depositStake(_rtcToStake, _durationInWeeks));
		emit TestPassed("deposit stake");
		assert(lockupI.verifyEnabledStake(address(this), 0));
		return true;
	}

}